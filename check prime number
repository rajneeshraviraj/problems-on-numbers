#include<bits/stdc++.h>
using namespace std;
bool checkPrime(int n){
    int cnt=0;
    for(int i=1;i<=n;i++)
    {
        if(n%i==0){
        cnt=cnt+1;
    }}
    if(cnt==2)
    {
        return true;
    }
    else{
        return false;
    }
    
}
int main(){
    int n=17;
    bool isPrime=checkPrime(n);
    if(isPrime)
    {
        cout<<"prime";
    }
    else{
        cout<<"not prime";
    }
    return 0;
}
